AddHandler cgi-script .cgi .pl
AddHandler server-parsed .shtml .html .htm
#AddType application/x-httpd-cgi
AddType text/html .shtml
AddType application/x-httpd-php .php
DirectoryIndex index.php index.html

php_value include_path "./:../:../../:../../../:../../../../:./common:../common:../../common:../../../common:../../../../common"
php_flag register_globals on
php_value  mbstring.internal_encoding    UTF-8
php_value "error_reporting" "E_ALL & ~E_NOTICE & ~E_DEPRECATED"

ErrorDocument 404 https://www.eye-r.com/user/

Options +FollowSymLinks
RewriteEngine On

RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]




RewriteRule ^user/screening/us([2-3])/$  user/screening/list.php?mode=stop&type=up&day=$1 [L,QSA]
RewriteRule ^user/screening/ds([2-3])/$  user/screening/list.php?mode=stop&type=dw&day=$1 [L,QSA]
RewriteRule ^user/screening/udc([3-5])/$  user/screening/list.php?mode=dc&type=up&day=$1 [L,QSA]
RewriteRule ^user/screening/ddc([3-5])/$  user/screening/list.php?mode=dc&type=dw&day=$1 [L,QSA]

RewriteRule ^bra([0-9]+)/$  user/page_brand.php?brand=$1 [L,QSA]

RewriteRule ^([0-9]{4})([0-9]{2})([0-9]{2})$  user/page.php?y=$1&m=$2&d=$3 [L,QSA]
RewriteRule ^([0-9]{4})([0-9]{2})([0-9]{2})/$  user/page.php?y=$1&m=$2&d=$3 [L,QSA]